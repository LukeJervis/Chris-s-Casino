{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/src/**/*.ts", "src/renderer/src/**/*.tsx", "src/preload/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    }
  }
}
